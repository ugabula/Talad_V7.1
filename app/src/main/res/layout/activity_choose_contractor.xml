<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".GetStartedActivity"
    android:background="@color/white">

    <ImageButton
        android:id="@+id/backarrow"
        android:layout_width="11dp"
        android:layout_height="20dp"
        android:src="@drawable/ic_blue_arrow_back"
        android:background="@color/white"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="10dp"
        android:layout_gravity="start|top"/>

    <TextView
        android:id="@+id/feedback"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/backarrow"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="42dp"
        android:layout_marginBottom="20dp"
        android:gravity="center"
        android:text="Please choose your contractor"
        android:textAlignment="center"
        android:textColor="@color/blue"
        android:textSize="20sp" />

    <ImageView
        android:layout_width="55dp"
        android:layout_height="48dp"
        android:src="@drawable/corn"
        android:layout_below="@+id/feedback"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="50dp"
        android:id="@+id/english" />

    <TextView
        android:id="@+id/corntext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/thaitext"
        android:layout_alignTop="@+id/english"
        android:text="Fabian Kostrhon"
        android:textSize="17sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/corntext"
        android:layout_below="@+id/corntext"
        android:text="฿800/Rai"
        android:textSize="13sp"/>

    <RatingBar
        android:id="@+id/ratingBar"
        style="?android:attr/ratingBarStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView11"
        android:layout_alignStart="@+id/ratingBar1"
        android:isIndicator="true"
        android:numStars="5"
        android:rating="@color/talad_green"
        android:stepSize="0.1" />

    <RadioButton
        android:id="@+id/radioButton_en"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/corntext"
        android:layout_alignBottom="@+id/corntext"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="19dp"
        android:shadowColor="@color/blue"/>

    <ImageView
        android:id="@+id/cass"
        android:layout_width="55dp"
        android:layout_height="48dp"
        android:src="@drawable/cassava"
        android:layout_below="@+id/english"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="20dp"/>

    <TextView
        android:id="@+id/thaitext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/cass"
        android:layout_marginStart="23dp"
        android:layout_toEndOf="@+id/cass"
        android:text="Phillip Fricke"
        android:textSize="17sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/thaitext"
        android:layout_below="@+id/thaitext"
        android:text="฿800/Rai"
        android:textSize="13sp"/>

    <RatingBar
        android:id="@+id/ratingBar1"
        style="?android:attr/ratingBarStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/ratingBar4"
        android:layout_below="@+id/thaitext"
        android:isIndicator="true"
        android:numStars="5"
        android:rating="@color/talad_green"
        android:stepSize="0.1" />

    <RadioButton
        android:id="@+id/radioButton_thai"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/thaitext"
        android:layout_alignBottom="@+id/thaitext"
        android:layout_alignStart="@+id/radioButton_en"
        android:shadowColor="@color/blue" />

    <ImageView
        android:id="@+id/miel"
        android:layout_width="55dp"
        android:layout_height="48dp"
        android:src="@drawable/mielies"
        android:layout_below="@+id/cass"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="20dp"/>

    <TextView
        android:id="@+id/mieltext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/miel"
        android:layout_marginStart="23dp"
        android:layout_toEndOf="@+id/miel"
        android:text="Manop Thapana"
        android:textSize="17sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/mieltext"
        android:layout_below="@+id/mieltext"
        android:text="฿800/Rai"
        android:textSize="13sp"/>

    <RatingBar
        android:id="@+id/ratingBar2"
        style="?android:attr/ratingBarStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView13"
        android:layout_alignStart="@+id/ratingBar1"
        android:isIndicator="true"
        android:numStars="5"
        android:rating="@color/talad_green"
        android:stepSize="0.1" />

    <RadioButton
        android:id="@+id/radioButton_miel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/mieltext"
        android:layout_alignBottom="@+id/mieltext"
        android:layout_alignStart="@+id/radioButton_en"
        android:shadowColor="@color/blue" />

    <ImageView
        android:id="@+id/rice"
        android:layout_width="55dp"
        android:layout_height="48dp"
        android:src="@drawable/rice"
        android:layout_below="@+id/miel"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="20dp"/>

    <TextView
        android:id="@+id/ricetext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/rice"
        android:layout_marginStart="23dp"
        android:layout_toEndOf="@+id/rice"
        android:text="Kebone Moloko"
        android:textSize="17sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/ricetext"
        android:layout_below="@+id/ricetext"
        android:text="฿800/Rai"
        android:textSize="13sp"/>

    <RatingBar
        android:id="@+id/ratingBar3"
        style="?android:attr/ratingBarStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView14"
        android:layout_alignStart="@+id/ratingBar4"
        android:isIndicator="true"
        android:numStars="5"
        android:rating="@color/talad_green"
        android:stepSize="0.1" />

    <RadioButton
        android:id="@+id/radioButton_rice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/ricetext"
        android:layout_alignBottom="@+id/ricetext"
        android:layout_alignStart="@+id/radioButton_en"
        android:shadowColor="@color/blue" />

    <ImageView
        android:id="@+id/pine"
        android:layout_width="55dp"
        android:layout_height="48dp"
        android:src="@drawable/pineapple"
        android:layout_below="@+id/rice"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="20dp"/>

    <TextView
        android:id="@+id/pinetext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/pine"
        android:layout_marginStart="23dp"
        android:layout_toEndOf="@+id/rice"
        android:text="James Smith"
        android:textSize="17sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/pinetext"
        android:layout_below="@+id/pinetext"
        android:text="฿800/Rai"
        android:textSize="13sp"/>

    <RatingBar
        android:id="@+id/ratingBar4"
        style="?android:attr/ratingBarStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/pinetext"
        android:layout_marginStart="13dp"
        android:layout_toEndOf="@+id/textView15"
        android:isIndicator="true"
        android:numStars="5"
        android:rating="@color/talad_green"
        android:stepSize="0.1" />

    <RadioButton
        android:id="@+id/radioButton_pine"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/pinetext"
        android:layout_alignBottom="@+id/pinetext"
        android:layout_alignStart="@+id/radioButton_en"
        android:shadowColor="@color/blue" />

</RelativeLayout>
