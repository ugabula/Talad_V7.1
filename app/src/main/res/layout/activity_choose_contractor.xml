<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".GetStartedActivity"
    android:background="@color/white">

    <ImageButton
        android:id="@+id/backarrow"
        android:layout_width="11dp"
        android:layout_height="20dp"
        android:src="@drawable/ic_blue_arrow_back"
        android:background="@color/white"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="10dp"
        android:layout_gravity="start|top"/>

    <TextView
        android:id="@+id/feedback"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/backarrow"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="42dp"
        android:layout_marginBottom="20dp"
        android:gravity="center"
        android:text="Please choose your contractor"
        android:textAlignment="center"
        android:textColor="@color/blue"
        android:textSize="20sp" />

    <ImageView
        android:layout_width="55dp"
        android:layout_height="48dp"
        android:src="@drawable/corn"
        android:layout_below="@+id/feedback"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="50dp"
        android:id="@+id/english" />

    <TextView
        android:id="@+id/corntext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/thaitext"
        android:layout_alignTop="@+id/english"
        android:text="Fabian Kostrhon "
        android:textSize="17sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/corntext"
        android:layout_below="@+id/corntext"
        android:text="฿800/Rai"
        android:textSize="13sp"/>

    <ImageView
        android:id="@+id/imageView30"
        android:layout_width="14dp"
        android:layout_height="13dp"
        android:layout_below="@+id/corntext"
        android:layout_marginStart="11dp"
        android:layout_toEndOf="@+id/textView11"
        app:srcCompat="@drawable/star" />

    <ImageView
        android:id="@+id/imageView31"
        android:layout_width="14dp"
        android:layout_height="13dp"
        android:layout_below="@+id/corntext"
        android:layout_toEndOf="@+id/imageView30"
        app:srcCompat="@drawable/star" />

    <ImageView
        android:id="@+id/imageView32"
        android:layout_width="14dp"
        android:layout_height="13dp"
        android:layout_below="@+id/corntext"
        android:layout_toEndOf="@+id/imageView31"
        app:srcCompat="@drawable/star" />

    <ImageView
        android:id="@+id/imageView33"
        android:layout_width="14dp"
        android:layout_height="13dp"
        android:layout_below="@+id/corntext"
        android:layout_toEndOf="@+id/imageView32"
        app:srcCompat="@drawable/star" />

    <ImageView
        android:id="@+id/imageView34"
        android:layout_width="14dp"
        android:layout_height="13dp"
        android:layout_below="@+id/corntext"
        android:layout_toEndOf="@+id/imageView33"
        app:srcCompat="@drawable/fade_star" />

    <RadioButton
        android:id="@+id/radioButton_en"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/corntext"
        android:layout_alignBottom="@+id/corntext"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="19dp"
        android:shadowColor="@color/blue"/>

    <ImageView
        android:id="@+id/cass"
        android:layout_width="55dp"
        android:layout_height="48dp"
        android:src="@drawable/cassava"
        android:layout_below="@+id/english"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="20dp"/>

    <TextView
        android:id="@+id/thaitext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/cass"
        android:layout_marginStart="23dp"
        android:layout_toEndOf="@+id/cass"
        android:text="Phillip Fricke"
        android:textSize="17sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView12"
